    public override void Input0_ProcessInputRow(Input0Buffer Row)
    {
        bool result = false;
        try
        {
            string url = this.Variables.APIURL + "/company";
            
            // arrange data
            object content = new
            {
                ID = Row.Id,
                Change = Row.Change,
                OrgOrSecurityNo = Row.OrgOrSecurityNo,
                FirstName = Row.FirstName,
                OrgOrLastName = Row.OrgOrLastName,
                IsActive = Row.IsActive
            };

            JavaScriptSerializer serielizer = new JavaScriptSerializer();

            var json = serielizer.Serialize(content);

            Console.WriteLine(json);

            var data = new StringContent(json, Encoding.UTF8, "application/json");

            //make request , use sync 
            HttpClient httpClient = new HttpClient();
            var response = httpClient.PostAsync(url, data).Result;

            // get response, use sync 
            if (response.IsSuccessStatusCode)
            {
                //Dts.TaskResult = (int)ScriptResult.Success;

                Console.WriteLine("Request Success");
                bool pbFireAgain = false;
                ComponentMetaData.FireInformation(0, "Api Sucess", response.ReasonPhrase, response.Content.ReadAsStringAsync().Result,0, pbFireAgain);
            }
            else
            {
                Console.WriteLine("Request Failed");
                Console.WriteLine(response.ReasonPhrase);
                ComponentMetaData.FireError(0, "An error occured", response.ReasonPhrase, response.Content.ReadAsStringAsync().Result, 0, out result);
            }
        }
        catch (Exception e)
        {
            string message = e.Message;
            string description = e.StackTrace;

            if (e.InnerException != null) 
            {
                message += " ( " + e.InnerException.Message + " )";
                description += " ( " + e.InnerException.StackTrace + " )";

            }

            ComponentMetaData.FireError(0, "An error occured", message, description, 0, out result);
        }
    }

